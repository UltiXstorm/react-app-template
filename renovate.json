{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "branchConcurrentLimit": 5,
  "branchPrefix": "renovate-",
  "packageRules": [{
    "commitMessagePrefix": "chore(dependencies): ",
    "matchUpdateTypes": ["minor","patch", "pin", "digest"],
    "automerge": true
  }]
}
